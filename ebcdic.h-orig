/* Control characters: */
#define	NUL	0x0		/* Null */
#define	SOH	0x1		/* Start of Header */
#define	STX	0x2		/* Start of text */
#define	DLE	0x10		/* Data Link Escape */
#define	ETB	0x26		/* End of Transmission Block */
#define	ENQ	0x2d		/* Enquire */
#define	SYN	0x32		/* Synchronization character */
#define	NAK	0x3d		/* Negative Ack */
#define	ACK0	0x70		/* Positive ACK */
#define	PAD	0xff		/* Pad character */

/* The written characters */
#define E_MINUS 0x60	/* - */
#define	E_PLUS	0x4e	/* + */
#define	E_SP	0x40
#define	E_A	0xc1
#define	E_B	0xc2
#define	E_C	0xc3
#define	E_D	0xc4
#define	E_E	0xc5
#define	E_F	0xc6
#define	E_G	0xc7
#define	E_H	0xc8
#define	E_I	0xc9
#define	E_J	0xd1

#define	E_M	0xd4
#define	E_N	0xd5
#define	E_R	0xd9
#define	E_Y	0xe8

#define	E_0	0xf0
#define	E_1	0xf1
#define	E_2	0xf2
#define	E_3	0xf3
#define	E_4	0xf4
#define	E_5	0xf5
#define	E_6	0xf6
#define	E_7	0xf7
#define	E_8	0xf8
#define	E_9	0xf9

